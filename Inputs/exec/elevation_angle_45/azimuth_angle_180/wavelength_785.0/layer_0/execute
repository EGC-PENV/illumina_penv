#!/bin/sh
#SBATCH --job-name=Illumina
#SBATCH --time=1:00:00
#SBATCH --mem=2G
cd /home/hlinares/git/illumina/Inputs/exec/elevation_angle_45/azimuth_angle_180/wavelength_785.0/layer_0
