mv Hawaii.out Hawaii_elevation_angle_45-azimuth_angle_180-wavelength_785.0-layer_0.out
mv Hawaii_pcl.bin Hawaii_pcl_elevation_angle_45-azimuth_angle_180-wavelength_785.0-layer_0.bin
