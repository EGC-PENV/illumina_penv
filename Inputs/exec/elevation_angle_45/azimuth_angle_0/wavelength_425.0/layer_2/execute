mv Hawaii.out Hawaii_elevation_angle_45-azimuth_angle_0-wavelength_425.0-layer_2.out
mv Hawaii_pcl.bin Hawaii_pcl_elevation_angle_45-azimuth_angle_0-wavelength_425.0-layer_2.bin
